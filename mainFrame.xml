<Ui xmlns="http://www.blizzard.com/wow/ui/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
  <Script file="main.lua"/>
  <Frame name="RLT_Frame" parent="UIParent">
    <Size x="100" y="32" />
    <Anchors>
      <Anchor point="CENTER" />
    </Anchors>
    <Frames>
      <Button name="RLT_Button" inherits="UIPanelButtonTemplate">
        <Size x='16' y='16' />
        <Anchors>
          <Anchor point="CENTER" />
        </Anchors>
      </Button>
    </Frames>
    <Scripts>
      <OnLoad>
        RLT_ShowMessage();
      </OnLoad>
    </Scripts>
    <Backdrop bgFile="Interface\TutorialFrame\TutorialFrameBackground" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <EdgeSize val="16"/>
      <TileSize val="32"/>
      <BackgroundInsets left="5" right="5" top="5" bottom="5"/>
    </Backdrop>
  </Frame>
</Ui>